# Copy this file to `.env` in the project root and fill in your keys.
# The .env file is listed in pubspec assets and required for builds.

# Google Generative Language API (Gemini)
GEMINI_API=

# Google Maps Platform APIs (Places, Geocoding, Directions)
GOOGLE_API_KEY=

# OpenWeatherMap API (for weather)
OPENWEATHER_API_KEY=

# Backend service (optional; used if configured)
API_BASE=
BACKEND_API_KEY=

# Blockchain / Web3
RPC_URL=
CONTRACT_ADDRESS=
CHAIN_ID=

# IPFS / Pinata (optional)
PINATA_JWT=
IPFS_PRIMARY_GATEWAY=https://gateway.pinata.cloud/ipfs/
IPFS_FALLBACK_GATEWAY=https://ipfs.io/ipfs/
# Comma-separated additional gateways, e.g. https://cloudflare-ipfs.com/ipfs/,https://dweb.link/ipfs/
IPFS_EXTRA_GATEWAYS=
